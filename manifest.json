{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Bookmark Homepage",
    "short_name": "BMHP",
    "description": "Your Bookmark Homepage",
    "version": "1",

    "offline_enabled": true,
    "manifest_version": 2,
    "permissions": [
        "bookmarks",
        "topSites",
        "storage",
        "chrome://favicon/",
        "https://*/*",
        "http://*/*"
    ],
    "chrome_url_overrides": {
        "newtab": "main.html"
    }
}